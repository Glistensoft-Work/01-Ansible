{
  "checks": {
     "check_mysql_alive": {
        "command": "check-mysql-alive.rb -h :::mysql.host::: -P :::mysql.port::: -u :::mysql.user::: -p :::mysql.password::: -d :::mysql.database::: -s :::mysql.socket:::",
        "handlers": ["default","mailer"],
        "subscribers": [
           "check-mysql-alive"
              ],
        "interval": 60,
        "occurances": 20
      }
  }
}
